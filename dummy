public class AddTwoNumbers {
    public static void main(String[] args) {
        // Define two numbers
        int num1 = 10; // You can change this value
        int num2 = 20; // You can change this value

        // Calculate the sum
        int sum = num1 + num2;

        // Display the result
        System.out.println("The sum of " + num1 + " and " + num2 + " is: " + sum);
    }
}

  
